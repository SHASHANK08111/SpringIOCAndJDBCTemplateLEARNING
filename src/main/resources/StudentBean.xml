<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans.xsd">
 
  <!-- Dependency Injection Using Setter -->   
  <bean id="Student1" class="com.example.demo.DependecyInjection.Student">
	 <property name="id" value="1"/> 
	 <property name="studentName" value="Raj"/> 
  </bean>
  
  
   <!-- Dependency Injection Using Constructor --> 
    <bean id="Student2" class="com.example.demo.DependecyInjection.Student">
	  <constructor-arg name="id" value="1"/>
	  <constructor-arg name="studentName" value="Raj"/>
    </bean>  
 
  
</beans>